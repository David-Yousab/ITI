pipeline {
    agent any

    stages {
        stage('Check') {
            steps {
                echo "Checking the code"
            }
        }

        stage('Test') {
            steps {
                echo "Testing the code"
            }
        }

        stage('Deployment') {
            steps {
                echo "Deploying the code"
            }
        }

        stage('testing the webhook') {
            steps {
                echo "Done test"
            }
        }
    }
}
